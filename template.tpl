const log = require('logToConsole');
const Object = require('Object');
const setDefaultConsentState = require('setDefaultConsentState');
const updateConsentState = require('updateConsentState');
const injectScript = require('injectScript');
const encodeUri = require('encodeUri');
const getCookieValues = require('getCookieValues');
const callInWindow = require('callInWindow');
const setInWindow = require('setInWindow');
const gtagSet = require('gtagSet');
const JSON = require('JSON');
const epCookieName = 'enkeltPersonvernConsent';

log('data =', data);

/*
 *   Splits the input string using comma as a delimiter, returning an array of
 *   strings
 */
const splitInput = (input) => {
  return input.split(',')
      .map(entry => entry.trim())
      .filter(entry => entry.length !== 0);
};
/*
 *   Processes a row of input from the default settings table, returning an object
 *   which can be passed as an argument to setDefaultConsentState
 */
const parseDefaultSettings = (settings) => {
  const regions = splitInput(settings.region);
  const commandData = {
    ad_storage: settings.ad_storage,
    ad_user_data: settings.ad_user_data,
    ad_personalization: settings.ad_personalization,
    analytics_storage: settings.analytics_storage,
    functionality_storage: settings.functionality_storage,
    personalization_storage: settings.personalization_storage,
    security_storage: settings.security_storage,
  };
  if (regions.length > 0) {
    commandData.region = regions;
  }
  return commandData;
};

const mapConsentCategoriesToGTM = (consent) => {
  const mapBooleanToGTM = (value) => (value ? "granted" : "denied");

  return {
    ad_storage: mapBooleanToGTM(consent.marketing || false),
    ad_user_data: mapBooleanToGTM(consent.personalization || false),
    ad_personalization: mapBooleanToGTM(consent.personalization || false),
    analytics_storage: mapBooleanToGTM(consent.analytics || false),
    functionality_storage: mapBooleanToGTM(consent.necessary || false),
    personalization_storage: mapBooleanToGTM(consent.personalization || false),
    security_storage: mapBooleanToGTM(consent.necessary || false),
  };
};

/*
 *   Called when consent changes. Assumes that consent object contains keys which
 *   directly correspond to Google consent types.
 */
const onUserConsent = (consent) => {
  updateConsentState(consent);
};

// Run script to show cookie banner
const onInjectSuccess = () => {
  log('Enkelt Personvern script injected successfully by GTM.');
  callInWindow('enkeltPersonvern.addConsentListener', onUserConsent);
  data.gtmOnSuccess();
};

/* If the script fails to inject successfully, log a message and signal failure */
const onInjectFailure = () => {
  log('Enkelt Personvern GTM script injection failed.');
  data.gtmOnFailure();
};

/*
 *   Executes the default command, sets the developer ID, and sets up the consent
 *   update callback
 */
const main = (data) => {
  /*
   * Optional settings using gtagSet
   */
  gtagSet('ads_data_redaction', data.ads_data_redaction);
  gtagSet('url_passthrough', data.url_passthrough);
  gtagSet('developer_id.dMGFkNz', true);
  
  // Store data on site which our injected script will use
  setInWindow("epSiteId", data.epSiteId, true);
  setInWindow("epDomain", data.epDomain, true);
  
  
  // Set default consent state(s)
  if (data.defaultSettings && data.defaultSettings.length > 0) {
    data.defaultSettings.forEach(settings => {
      const defaultData = parseDefaultSettings(settings);
      // wait_for_update (ms) allows for time to receive visitor choices from the CMP
      defaultData.wait_for_update = 500;
      setDefaultConsentState(defaultData);
    });
  } else {
    // Use sensible defaults
    setDefaultConsentState({
    ad_storage: 'denied',
    analytics_storage: 'denied',
    ad_user_data: 'denied',
    ad_personalization: 'denied',
    functionality_storage: 'granted',
    personalization_storage: 'denied',
    security_storage: 'granted',
    wait_for_update: 500
  });
  }
  
  const cookieValue = getCookieValues(epCookieName);

  if (cookieValue && cookieValue.length > 0) {
    // Ensure cookieValue is a valid JSON string
    const parsedCookie = JSON.parse(cookieValue);

    if (parsedCookie && Object.keys(parsedCookie).length > 0 && parsedCookie.consent) {
      const consent = parsedCookie.consent;
      const gtmConsent = mapConsentCategoriesToGTM(consent);
      onUserConsent(gtmConsent);
    } else {
      log("GTM WARNING: Consent object not found or cookie format is invalid.");
    }
  } else {
    log("No previous enkeltpersonvern.no consent found");
  }
  
  
  
  const url = encodeUri('https://' + data.epDomain + '/v1/' + data.epSiteId + '/gtm-banner/');
  injectScript(url, onInjectSuccess, onInjectFailure, url);
  
 
};
main(data);


